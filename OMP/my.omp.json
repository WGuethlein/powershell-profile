{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u256d",
          "foreground": "red",
          "type": "text",
          "style": "plain"
        },
        {
          "leading_diamond": "\ue0b6",
          "foreground": "lightRed",
          "background": "#1d2021",
          "type": "session",
          "style": "diamond"
        },
        {
          "options": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "foreground": "lightGreen",
          "powerline_symbol": "\ue0b0",
          "background": "#282828",
          "type": "git",
          "style": "powerline"
        },
        {
          "foreground": "lightYellow",
          "powerline_symbol": "\ue0b0",
          "background": "#3c3836",
          "type": "shell",
          "style": "powerline"
        },
        {
          "foreground": "lightYellow",
          "powerline_symbol": "\ue0b0",
          "background": "#3c3836",
          "type": "root",
          "style": "powerline"
        },
        {
          "options": {
            "threshold": 0
          },
          "foreground": "lightBlue",
          "powerline_symbol": "\ue0b0",
          "background": "#504945",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570",
          "foreground": "red",
          "type": "text",
          "style": "plain"
        },
        {
          "options": {
            "style": "full"
          },
          "leading_diamond": "\ue0b6",
          "foreground": "lightCyan",
          "background": "#1d2021",
          "type": "path",
          "style": "diamond"
        },
        {
          "options": {
            "always_enabled": true
          },
          "foreground": "green",
          "powerline_symbol": "\ue0b0",
          "background": "#282828",
          "type": "exit",
          "style": "powerline",
          "foreground_templates": [
            "{{ if gt .Code 0 }}lightWhite{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "az"
    }
  ],
  "version": 4
}
